// UBUNTU

@font-face {
    font-family: 'ubuntu';
    src: url(<%= asset_path('ubuntu-l-webfont.eot') %>);
    src: url(<%= asset_path('ubuntu-l-webfont.eot?#iefix') %>) format('embedded-opentype'),
         url(<%= asset_path('ubuntu-l-webfont.woff') %>) format('woff'),
         url(<%= asset_path('ubuntu-l-webfont.ttf') %>) format('truetype'),
         url(<%= asset_path('ubuntu-l-webfont.svg#ubuntulight') %>) format('svg');
    font-weight: 200;
    font-style: normal;

}

