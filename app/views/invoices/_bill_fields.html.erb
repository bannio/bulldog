<tr>
  <td class="first-col"><%= f.date %></td>
  <td><%= f.supplier_name %></td>
  <td><%= f.category_name %></td>
  <td><%= f.description %></td>
  <td class="align-right"><%= number_to_currency(f.amount) %></td>
  <% if params[:action] == 'edit' %>
    <td><%= check_box_tag "bill_ids[]", f.id %></td>
  <% end %>
</tr>
