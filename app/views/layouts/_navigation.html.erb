<!-- FIXED TOP NAVBAR -->
        <header class="row">
            <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
                    <!-- NAV TOGGLING -->
                    <div class="navbar-header col-xs-8">
                        <button type="button" class="navbar-toggle pull-left" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>    
                    </div><!--end navbar toggling-->    
                   
                    <!-- NAVBAR ITEMS -->
                    <div class="navbar-items">
                      <div class="navbar-links navbar-collapse collapse col-sm-8">
                          <ul class="nav navbar-nav">
                            <%= render 'layouts/navigation_links' %>
                          </ul>
                      </div><!--end navbar-collapse-->

                      <!-- NAVBAR LOGO -->
                      <a class="navbar-logo nav navbar-nav navbar-brand navbar-right col-xs-4" href="#">
                          <img src="/assets/navbar_logo.png" class="img-responsive" alt="navbar logo">
                      </a><!--end navbar logo--> 
                    </div>
            </nav> <!--end navbar-->           
        </header> <!--end header row-->
